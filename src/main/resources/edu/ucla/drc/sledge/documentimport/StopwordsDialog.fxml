<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="edu.ucla.drc.sledge.documentimport.StopwordsDialog"
            prefHeight="400.0" prefWidth="600.0">

    <TableView>
        <columns>
            <TableColumn text="Word">
                <cellValueFactory>
                    <PropertyValueFactory property="word"/>
                </cellValueFactory>
            </TableColumn>
            <TableColumn text="Actions">
                <cellValueFactory>
                    <PropertyValueFactory property="dummy"/>
                </cellValueFactory>
            </TableColumn>
        </columns>

    </TableView>

</AnchorPane>
