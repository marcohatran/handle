<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.control.cell.PropertyValueFactory?>
<fx:root
        xmlns="http://javafx.com/javafx"
         xmlns:fx="http://javafx.com/fxml"
         prefHeight="400.0" prefWidth="600.0"
        type="AnchorPane"
>
    <TableView fx:id="wordTable">
        <columns>
            <TableColumn text="Word" fx:id="wordColumn">
            </TableColumn>
            <TableColumn text="Count"  fx:id="countColumn">
            </TableColumn>
        </columns>

        <sortOrder>
            <fx:reference source="wordColumn" />
        </sortOrder>
    </TableView>

</fx:root>
